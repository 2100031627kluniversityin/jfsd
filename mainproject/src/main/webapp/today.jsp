    <script>
        function toggleInfo(element) {
            const info = element.querySelectorAll('p');
            for (let i = 0; i < info.length; i++) {
                if (info[i].style.display === 'none') {
                    info[i].style.display = 'block';
                } else {
                    info[i].style.display = 'none';
                }
            }
        }
    </script>
